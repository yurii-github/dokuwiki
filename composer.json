{
  "name": "my project",
  "description": "DokuWiki is a simple to use and highly versatile Open Source wiki software that doesn't require a database",
  "homepage": "https://www.dokuwiki.org",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.0",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "geshi/geshi": "^1.0",
    "incenteev/composer-parameter-handler": "^2.0",
    "monolog/monolog": "^1.23",
    "openpsa/universalfeedcreator": "^1.8",
    "paragonie/random_compat": "^2.0",
    "phpseclib/phpseclib": "~2.0",
    "sensio/distribution-bundle": "^5.0",
    "marcusschwarz/lesserphp": "^0.5.1",
    "sensio/framework-extra-bundle": "^3.0",
    "simplepie/simplepie": "^1.4",
    "splitbrain/php-archive": "~1.0",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/dependency-injection": "^3.3",
    "symfony/monolog-bundle": "^3.1",
    "symfony/swiftmailer-bundle": "^3.0",
    "symfony/symfony": "^3.4@dev",
    "symfony/yaml": "^3.3",
    "sensio/generator-bundle": "^3.0"
  },
  "suggest": {
    "squizlabs/php_codesniffer": "PHP_CodeSniffer tokenizes PHP, JavaScript and CSS files and detects violations of a defined set of coding standards.",
    "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
  },
  "autoload": {
    "psr-4": {
      "Yurii\\": "Yurii/src/",
      "dokuwiki\\Service\\": "inc/Service/",
      "AppBundle\\": "symfony-app/src/AppBundle"
    },
    "classmap": [
      "symfony-app/app/AppKernel.php",
      "symfony-app/app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "symfony-app/tests/"
    },
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  },
  "prefer-stable": true,
  "require-dev": {
    "mikey179/vfsStream": "^1.6",
    "symfony/phpunit-bridge": "^3.0"
  },

  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "symfony-app-dir": "symfony-app/app",
    "symfony-bin-dir": "symfony-app/bin",
    "symfony-var-dir": "symfony-app/var",
    "symfony-web-dir": "symfony-app/web",
    "symfony-tests-dir": "symfony-app/tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "symfony-app/app/config/parameters.yml"
    },
    "branch-alias": null
  }


}
